.tel-template {
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, .3);
  top: 0;
  left: 0;
}
.tel-template .info {
  position: absolute;
  left: 0;
  bottom: -362rpx;
  width: 100%;
  font-size: 30rpx;
  line-height: 88rpx;
  animation: showTel .3s ease-out forwards;
}
.tel-template .hideTelClass {
  animation: hideTel .3s ease-out forwards;
}
@keyframes showTel
{
  from  { bottom: -362rpx; }
  to    { bottom: 0; }
}
@keyframes hideTel
{
  from  { bottom: 0; }
  to    { bottom: -362rpx; }
}
.tel-template .title {
  height: 88rpx;
  background-color: #eeeeee;
  text-align: center;
  font-weight: 700;
}
.tel-template .cancel {
  height: 88rpx;
  background-color: #fff;
  text-align: center;
}
.tel-template .content {
  background-color: #fff;
  margin-bottom: 10rpx;
}
.tel-template .content view {
  padding: 0 30rpx;
  position: relative;
}
.tel-template .content view:first-child {
  border-bottom: 1px solid #e5e5e5;
}
.tel-template .content image {
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  width: 16rpx;
  height: 28rpx;
}
.tel-template .content .tit {
  font-size: 24rpx;
  color: #848484;
  margin-right: 20rpx;
  padding-left: 0;
}